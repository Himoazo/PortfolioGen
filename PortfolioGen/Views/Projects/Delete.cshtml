@model PortfolioGen.Models.Project

@{
    ViewData["Title"] = "Delete";
}


<h3 class="row justify-content-center">Är du säker att du vill ta bort projektet: @Html.DisplayFor(model => model.Title)?</h3>
<div> 
    <form asp-action="Delete" class="text-center">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a asp-action="Index" class="btn btn-secondary">Tillbaka</a>
    </form>
</div>
